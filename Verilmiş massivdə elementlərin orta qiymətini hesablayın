using System;

class Program
{
    static void Main()
    {
        int[] arr = { 10, 20, 30, 40, 50 };
        int sum = 0;

        foreach (int item in arr)
        {
            sum += item;
        }

        double average = (double)sum / arr.Length;
        Console.WriteLine($"Massivin orta qiym…ôti: {average:F2}");
    }
}
