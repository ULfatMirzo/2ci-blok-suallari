using System;

class Program
{
    static void Main()
    {
        for (int i = 1; i <= 100; i++)
        {
            if (i > 50) // 50-dən sonra dayandırırıq
                break;

            if (i % 2 == 0) // cüt ədədləri ötürürük
                continue;

            Console.WriteLine(i);
        }
    }
}
