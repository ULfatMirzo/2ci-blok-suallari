using System;

class Program
{
    static void Main()
    {
        Console.Write("Bir mətn daxil edin: ");
        string text = Console.ReadLine();

        if (string.IsNullOrWhiteSpace(text))
            Console.WriteLine("Daxil edilən mətn boşdur.");
        else
            Console.WriteLine("Daxil edilən mətn boş deyil.");
    }
}
