using System;

class Program
{
    static void Main()
    {
        Console.Write("Yaşınızı daxil edin: ");
        int age = int.Parse(Console.ReadLine());

        if (age < 18)
            Console.WriteLine("Giriş qadağandır!");
        else
            Console.WriteLine("Xoş gəldiniz!");
    }
}
